printf("%d %d %d \n",10); //10 GV
printf("%d %d %d \n"); // GV GV GV
printf("%d %d %d \n",10,20,30,40); // 10 20 30
printf("%d %d %d \n",10,30.5,'4'); // 10 30.5000000 4
printf("%d %d %d \n",10+3); // 13
printf("%d + %d = %d \n",5,2,10); // 5 + 2 =10

printf("%d \n",7.5); // GV
printf("%d \n",5); // 5
printf(%d \n",5%2); // 1
printf(%d \n",5%-2); // 2
printf("%d",5/2); // invalid
printf("%d",5.0%2); // GV
printf("%d",5.0/2); // GV 10 20
printf("%d %d %d","%d %d ",10,20,30,40); // hello
printf("hello","hai","bye"); // hellohaibye
printf("""hello""); // hello
printf("""hello"""); // invalid
printf("10 20",%d); // 10 20
printf("10 %d","%d); // 10 GV

Relational operator
> < >= <= == !=

5 > 4 -> 1
5 > 5 -> 0
5 < 4 -> 0
5 < 5 -> 0
5 >= 4 -> 1
5 >= 5 -> 1
5 <= 4 -> 0
5 <= 5 -> 1
5 == 5 -> 1
5 != 5 -> 0
5 = 5 -- 1 value required
5> ; -- error


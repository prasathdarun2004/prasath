5/2=2
5/-2=-2
-5/2=-2
-5/-2=2

5.0/2=2.5
5/2.0=2.5
5.0/2.0=2.5
-5/2.0=2.5

2/5=0
2.0/5=0.4
2/5.0=0.4
2.0/5.0=0.4

+/+=+
+/-=-
-/+=-
-/-=+

-2/5=0
2/-5=0
-2/5.0=0.4

int/int=int
int/float=float
float/int=float
float/float=float

